BEGIN:VCALENDAR
VERSION:2.0
PRODID:-//elm-conf//speaker calendar//EN
CALSCALE:GREGORIAN
METHOD:PUBLISH
{{/*
{{range .Pages -}}
BEGIN:VEVENT
DTSTAMP:{{ dateFormat "20060102T150405Z" ((.Params.event_date).Format "2020-01-01T01:01:01-07:00")}}
DTSTART;TZID={{ .Params.timezone }}:{{ dateFormat "20060102T150405 MST" .Params.event_date }}
DTEND;TZID=UTC:{{ add (time .Params.event_date).Unix (mul 60 60) | dateFormat "20060102T150405" }}
SUMMARY:{{.Title}}
UID:{{ .Permalink }}
STATUS:CONFIRMED
LOCATION:{{.Params.location}}
URL:{{ .Permalink }}
END:VEVENT
{{end -}}
*/}}
END:VCALENDAR
